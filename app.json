{
  "expo": {
    "name": "bingoApp",
    "slug": "bingo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "CFBundleAllowMixedLocalizations": true,
        "ITSAppUsesNonExemptEncryption": false
      },
      "bundleIdentifier": "",
      "googleServicesFile": ""
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "",
      "googleServicesFile": ""
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "locales": {
      "pl": "./src/Assets/Languages/polish.json"
    },
    "plugins": [
      [
        "expo-font",
        {
          "fonts": [
            "./src/Assets/Fonts/Montserrat_Regular.ttf",
            "./src/Assets/Fonts/Montserrat_Bold.ttf",
            "./src/Assets/Fonts/Montserrat_Bold_Italic.ttf"
          ]
        }
      ],
      "expo-localization",
      [
        "expo-splash-screen",
        {
          "image": "./assets/splash-icon.png",
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "image": "./assets/splash-icon.png",
            "backgroundColor": "#ffffff"
          },
          "imageWidth": 200
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/icon.png",
          "defaultChannel": "default"
        }
      ]
    ],
    "owner": "verbum",
    "extra": {
      "apiUrl": "https://bingo.dev-verbum.pl/api/",
      "baseUrl": "https://bingo.dev-verbum.pl/",
      "apiKey": "hehe",
      "supportsRTL": true,
      "eas": {
        "projectId": "b3ec33f3-f4aa-4f60-9e63-98713c5850b0"
      }
    }
  }
}
